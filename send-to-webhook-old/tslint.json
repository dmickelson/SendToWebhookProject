{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false
  }
}
